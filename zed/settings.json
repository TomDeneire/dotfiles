{
    "language_models": {
        "openai": {
            "version": "1"
        }
    },
    "theme": "Spunkshui",
    "base_keymap": "VSCode",
    "features": {
        "inline_completion_provider": "supermaven"
    },
    "buffer_font_family": "FiraCode Nerd Font",
    "buffer_font_features": {
        "calt": false
    },
    "buffer_font_fallbacks": ["MesloLGL Nerd Font", "Zed Plex Mono"],
    "buffer_font_size": 30,
    "buffer_font_weight": 450,
    "buffer_line_height": "comfortable",
    "ui_font_family": "FiraCode Nerd Font",
    "ui_font_features": {
        "calt": false
    },
    "ui_font_weight": 450,
    "ui_font_size": 30,
    "active_pane_magnification": 1.0,
    "centered_layout": {
        "left_padding": 0.2,
        "right_padding": 0.2
    },
    "multi_cursor_modifier": "alt",
    "vim_mode": true,
    "hover_popover_enabled": true,
    "confirm_quit": false,
    "restore_on_startup": "last_session",
    "drop_target_size": 0.2,
    "when_closing_with_no_tabs": "platform_default",
    "cursor_blink": false,
    "current_line_highlight": "all",
    "show_completions_on_input": true,
    "show_completion_documentation": false,
    "completion_documentation_secondary_query_debounce": 300,
    "unnecessary_code_fade": 0.5,
    "show_wrap_guides": true,
    "wrap_guides": [80],
    "redact_private_values": false,
    "expand_excerpt_lines": 3,
    "private_files": [
        "**/.env*",
        "**/*.pem",
        "**/*.key",
        "**/*.cert",
        "**/*.crt",
        "**/secrets.yml"
    ],
    "use_on_type_format": true,
    "use_autoclose": false,
    "always_treat_brackets_as_autoclosed": false,
    "show_copilot_suggestions": true,
    "show_whitespaces": "selection",
    "calls": {
        "mute_on_join": false,
        "share_on_join": false
    },
    "toolbar": {
        "breadcrumbs": true,
        "quick_actions": true
    },
    "scrollbar": {
        "show": "auto",
        "cursors": true,
        "git_diff": true,
        "search_results": true,
        "selected_symbol": true,
        "diagnostics": true
    },
    "double_click_in_multibuffer": "select",
    "gutter": {
        "line_numbers": true,
        "code_actions": true,
        "folds": true
    },
    "search": {
        "whole_word": false,
        "case_sensitive": false,
        "include_ignored": true,
        "regex": false
    },
    "indent_guides": {
        "enabled": true,
        "line_width": 1,
        "coloring": "fixed",
        "background_coloring": "disabled"
    },
    "vertical_scroll_margin": 3,
    "scroll_sensitivity": 1.0,
    "relative_line_numbers": true,
    "seed_search_query_from_cursor": "always",
    "inlay_hints": {
        "enabled": false,
        "show_type_hints": true,
        "show_parameter_hints": true,
        "show_other_hints": true,
        "edit_debounce_ms": 700,
        "scroll_debounce_ms": 50
    },
    "project_panel": {
        "button": true,
        "default_width": 240,
        "dock": "left",
        "file_icons": true,
        "folder_icons": true,
        "git_status": true,
        "indent_size": 20,
        "auto_reveal_entries": true,
        "auto_fold_dirs": false
    },
    "outline_panel": {
        "button": false,
        "default_width": 240,
        "dock": "left",
        "file_icons": true,
        "folder_icons": true,
        "git_status": true,
        "indent_size": 20,
        "auto_reveal_entries": true,
        "auto_fold_dirs": true
    },
    "collaboration_panel": {
        "button": false,
        "dock": "left",
        "default_width": 240
    },
    "chat_panel": {
        "button": false,
        "dock": "right",
        "default_width": 240
    },
    "message_editor": {
        "auto_replace_emoji_shortcode": true
    },
    "notification_panel": {
        "button": true,
        "dock": "right",
        "default_width": 380
    },
    "assistant": {
        "default_model": {
            "provider": "openai",
            "model": "gpt-4o"
        },
        "version": "2",
        "enabled": true,
        "button": false,
        "dock": "right",
        "default_width": 640,
        "default_height": 640.0,
        "provider": null
    },
    "show_call_status_icon": false,
    "enable_language_server": true,
    "language_servers": ["gopls", "pyright", "LuaLS"],
    "autosave": "off",
    "tab_bar": {
        "show": true,
        "show_nav_history_buttons": true
    },
    "tabs": {
        "git_status": true,
        "file_icons": true,
        "close_position": "right"
    },
    "preview_tabs": {
        "enabled": true,
        "enable_preview_from_file_finder": true,
        "enable_preview_from_code_navigation": true
    },
    "remove_trailing_whitespace_on_save": true,
    "extend_comment_on_newline": true,
    "ensure_final_newline_on_save": true,
    "format_on_save": "on",
    "formatter": "language_server",
    "soft_wrap": "preferred_line_length",
    "soft_wrap_guides": [80],
    "preferred_line_length": 80,
    "hard_tabs": false,
    "tab_size": 4,
    "telemetry": {
        "diagnostics": false,
        "metrics": false
    },
    "auto_update": true,
    "diagnostics": {
        "include_warnings": true
    },
    "file_scan_exclusions": [
        "**/.git",
        "**/.svn",
        "**/.hg",
        "**/CVS",
        "**/.DS_Store",
        "**/Thumbs.db",
        "**/.classpath",
        "**/.settings"
    ],
    "git": {
        "git_gutter": "tracked_files",
        "inline_blame": {
            "enabled": false
        }
    },
    "copilot": {
        "disabled_globs": [".env"]
    },
    "journal": {
        "path": "~",
        "hour_format": "hour24"
    },
    "terminal": {
        "shell": { "program": "/bin/bash" },
        "dock": "bottom",
        "default_width": 640,
        "default_height": 520,
        "working_directory": "current_project_directory",
        "blinking": "terminal_controlled",
        "alternate_scroll": "off",
        "option_as_meta": false,
        "copy_on_select": true,
        "button": true,
        "env": {},
        "font_size": 30,
        "font_weight": 450,
        "font_family": "FiraCode Nerd Font",
        "line_height": "comfortable",
        "detect_venv": {
            "on": {
                "directories": [".env", "env", ".venv", "venv"],
                "activate_script": "default"
            }
        },
        "toolbar": {
            "title": true
        }
    },
    "code_actions_on_format": {},
    "file_types": {},
    "auto_install_extensions": {
        "html": true
    },
    "languages": {
        "Astro": {
            "prettier": {
                "allowed": true,
                "plugins": ["prettier-plugin-astro"]
            }
        },
        "Blade": {
            "prettier": {
                "allowed": true
            }
        },
        "C": {
            "format_on_save": "off"
        },
        "C++": {
            "format_on_save": "off"
        },
        "CSS": {
            "prettier": {
                "allowed": true
            }
        },
        "Elixir": {
            "language_servers": ["elixir-ls", "!next-ls", "!lexical", "..."]
        },
        "Go": {
            "code_actions_on_format": {
                "source.organizeImports": true
            }
        },
        "GraphQL": {
            "prettier": {
                "allowed": true
            }
        },
        "HEEX": {
            "language_servers": ["elixir-ls", "!next-ls", "!lexical", "..."]
        },
        "HTML": {
            "prettier": {
                "allowed": true
            }
        },
        "Java": {
            "prettier": {
                "allowed": true,
                "plugins": ["prettier-plugin-java"]
            }
        },
        "JavaScript": {
            "language_servers": ["typescript-language-server", "!vtsls", ".."],
            "prettier": {
                "allowed": true
            }
        },
        "JSON": {
            "language_servers": ["json-language-server"],
            "format_on_save": "true",
            "prettier": {
                "allowed": true
            }
        },
        "Lua": {
            "language_servers": ["lua-language-server", "lua-ls"]
        },
        "Markdown": {
            "format_on_save": "on",
            "language_servers": [""],
            "prettier": {
                "allowed": true
            }
        },
        "PHP": {
            "prettier": {
                "allowed": true,
                "plugins": ["@prettier/plugin-php"]
            }
        },
        "Ruby": {
            "language_servers": ["solargraph", "!ruby-lsp", "..."]
        },
        "Rust": {
            "language_servers": ["rust-analyzer"]
        },
        "SCSS": {
            "prettier": {
                "allowed": true
            }
        },
        "SQL": {
            "prettier": {
                "allowed": true,
                "plugins": ["prettier-plugin-sql"]
            }
        },
        "Svelte": {
            "prettier": {
                "allowed": true,
                "plugins": ["prettier-plugin-svelte"]
            }
        },
        "TSX": {
            "language_servers": ["typescript-language-server", "!vtsls", ".."],
            "prettier": {
                "allowed": true
            }
        },
        "Twig": {
            "prettier": {
                "allowed": true
            }
        },
        "ReStructuredTxt": {
            "language_servers": ["swyddfa/esbonio", "esbonio"],
            "prettier": {
                "allowed": true
            }
        },
        "TypeScript": {
            "language_servers": ["typescript-language-server", "!vtsls", ".."],
            "prettier": {
                "allowed": true
            }
        },
        "Vue.js": {
            "prettier": {
                "allowed": true
            }
        },
        "XML": {
            "prettier": {
                "allowed": true,
                "plugins": ["@prettier/plugin-xml"]
            }
        },
        "YAML": {
            "prettier": {
                "allowed": true
            }
        }
    },
    "prettier": {},
    "lsp": {
        "rust-analyzer": {}
    },
    "vim": {
        "use_system_clipboard": "always",
        "use_multiline_find": false,
        "use_smartcase_find": false
    },
    "task": {
        "show_status_indicator": true
    },
    "line_indicator_format": "long"
}
